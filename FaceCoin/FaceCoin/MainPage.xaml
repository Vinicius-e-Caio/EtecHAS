<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://schemas.microsoft.com/dotnet/2021/maui"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FaceCoin.MainPage">

    <ScrollView>
        <VerticalStackLayout
            Padding="30,0"
            Spacing="25">

            <Image Source="data:image/jpeg;base64," />

            <Picker x:Name="PickerChoice"
                Title="Selecione cara ou coroa">
                <Picker.ItemsSource>
                    <x:Array Type="{x:Type x:String}">
                        <x:String>Cara</x:String>
                        <x:String>Coroa</x:String>
                    </x:Array>
                </Picker.ItemsSource>
            </Picker>
            <Button
                x:Name="ButtonChoice"
                Text="Jogar a moeda"
                Clicked="ButtonChoice_Clicked"/>
        </VerticalStackLayout>
    </ScrollView>

</ContentPage>
